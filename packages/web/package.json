{
  "name": "@kurone-kito/kit.black-web",
  "version": "1.0.0",
  "private": true,
  "description": "the API fetcher for kit.black",
  "homepage": "https://github.com/kurone-kito/kit.black#readme",
  "bugs": "https://github.com/kurone-kito/kit.black/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurone-kito/kit.black.git",
    "directory": "packages/web"
  },
  "license": "CC-BY-NC-4.0",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "type": "module",
  "main": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "pnpm run \"/^prebuild:.+/\"",
    "build": "pnpm run \"/^build:.+/\"",
    "build:storybook": "storybook build",
    "build:vinxi": "vinxi build",
    "clean": "rimraf -g \"*.tgz\" \"*.tsbuildinfo\" .output .vinxi node_modules/.cache \"src/*.json\"",
    "prebuild:fetcher": "kb-fetcher > src/data.json",
    "prebuild:pkl": "pkl eval -f json src/constants.pkl -o src/constants.json",
    "serve": "serve .output/public",
    "prestart": "pnpm run prebuild",
    "start": "pnpm run -p \"/^start:.+/\"",
    "start:storybook": "storybook dev -p 6006",
    "start:vinxi": "vinxi dev",
    "test": "vitest run"
  },
  "dependencies": {
    "@kurone-kito/kit.black-lib": "workspace:^",
    "@solid-primitives/date": "^2.0.23",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.7",
    "@solidjs/start": "^1.0.8",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.2",
    "tailwind-merge": "^2.5.3"
  },
  "devDependencies": {
    "@kurone-kito/kit.black-fetcher": "workspace:^",
    "@kurone-kito/typescript-config": "^0.16.1",
    "@pkl-community/pkl": "0.25.1-2",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/docs-tools": "^8.3.5",
    "@storybook/types": "^8.3.5",
    "@tailwindcss/container-queries": "^0.1.1",
    "@testing-library/jest-dom": "^6.5.0",
    "autoprefixer": "^10.4.20",
    "daisyui": "^4.12.12",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "rimraf": "^6.0.1",
    "serve": "^14.2.3",
    "storybook": "^8.3.5",
    "storybook-solidjs": "1.0.0-beta.2",
    "storybook-solidjs-vite": "1.0.0-beta.2",
    "tailwindcss": "^3.4.13",
    "tailwindcss-safe-area": "^0.6.0",
    "type-fest": "^4.26.1",
    "typescript": "~5.6.2",
    "vinxi": "^0.4.3",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.2"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  }
}
